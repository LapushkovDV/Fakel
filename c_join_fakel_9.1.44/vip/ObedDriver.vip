  sResult : string;
{
  sResult := 'Удален';
  If (GetFirst KndriverTmp where ((cRec == KndriverTmp.Nrec)) = tsOk)
    sResult := KndriverTmp.Name;
  result := sResult;
}

Function CheckObj : boolean;
{
  #Check_Obj(KndriverTmp)
}

Procedure ObBreak;  // Прерывание объединения
var
  lcount_out : longint;
{
  lcount_out := VisualCount();
  message('Для прекращения объединения придется'
          + chr(13) + 'подождать до окончания замены на'
          + chr(13) + '"' + trim (KndriverTmp.Name) + '".', Information + OkButton);

  ReStartVisual('Идет замещение водителей на'#13'' + '"' + trim(KndriverTmp.Name)
                +'"' + ''#13'', m_lcount_vis);
  NNextVisual (lcount_out);
  m_bBreak := true;
}

